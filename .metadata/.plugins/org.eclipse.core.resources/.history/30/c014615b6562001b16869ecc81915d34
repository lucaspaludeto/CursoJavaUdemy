package fundamentos;

import java.util.Scanner;

public class Wrapper {
	
	public static void main(String[] args) {
		
		Scanner entrada = new Scanner(System.in);
		
		Byte b = 100;
		Short s = 1000;
		Integer i = Integer.parseInt("10000"); //int
		Long l = 100000L;
		
		entrada.close();
	}
}
